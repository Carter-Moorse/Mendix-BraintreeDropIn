<?xml version="1.0" encoding="utf-8"?>
<widget id="shelterbox.braintreedropin.BraintreeDropIn" pluginWidget="true" needsEntityContext="true" offlineCapable="true" supportedPlatform="Web"
  xmlns="http://www.mendix.com/widget/1.0/"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../node_modules/mendix/custom_widget.xsd">
  <name>Braintree Drop In</name>
  <description>Braintree drop-in implemented as Mendix pluggable widget</description>
  <icon>
  </icon>
  <properties>
    <propertyGroup caption="General">
      <property key="clientToken" type="attribute" required="true">
        <caption>Client token</caption>
        <description>INPUT: Token used to authorise the client</description>
        <attributeTypes>
          <attributeType name="String"/>
        </attributeTypes>
      </property>
      <property key="clientOptions" type="textTemplate" multiline="true">
        <caption>Options</caption>
        <description>INPUT: Braintree create options JSON (https://braintree.github.io/braintree-web-drop-in/docs/current/module-braintree-web-drop-in.html#.create)</description>
      </property>
      <property key="nonce" type="attribute" required="true">
        <caption>Client nonce</caption>
        <description>OUTPUT: Nonce gathered from the client after a successful purchase</description>
        <attributeTypes>
          <attributeType name="String"/>
        </attributeTypes>
      </property>
      <property key="deviceData" type="attribute" required="true">
        <caption>Device data</caption>
        <description>OUTPUT: Device data captured from the client for fraudulent detection</description>
        <attributeTypes>
          <attributeType name="String"/>
        </attributeTypes>
      </property>
    </propertyGroup>
    <propertyGroup caption="Style">
      <property key="submitButtonStyle" type="enumeration" defaultValue="default">
        <caption>Submit button style</caption>
        <description></description>
        <enumerationValues>
          <enumerationValue key="default">Default</enumerationValue>
          <enumerationValue key="primary">Primary</enumerationValue>
          <enumerationValue key="success">Success</enumerationValue>
          <enumerationValue key="info">Info</enumerationValue>
          <enumerationValue key="inverse">Inverse</enumerationValue>
          <enumerationValue key="warning">Warning</enumerationValue>
          <enumerationValue key="danger">Danger</enumerationValue>
        </enumerationValues>
      </property>
      <property key="submitButtonText" type="string" required="false">
        <caption>Submit button text</caption>
        <description>The braintreedropin value shown when no value is provided via the attribute</description>
      </property>
    </propertyGroup>
    <propertyGroup caption="Events">
      <property key="onFormSubmit" type="action" required="false">
        <caption>On form submit action</caption>
        <description>Action to trigger when submit button is clicked and form is successfully processed</description>
      </property>
      <property key="onCreate" type="action" required="false">
        <caption>On form create</caption>
        <description>Action to trigger when form is created</description>
      </property>
      <property key="onDestroyStart" type="action" required="false">
        <caption>On form destroy start</caption>
        <description>Action to trigger when form starts destuction</description>
      </property>
      <property key="onDestroyEnd" type="action" required="false">
        <caption>On form destroy end</caption>
        <description>Action to trigger when form ends destruction</description>
      </property>
      <property key="onError" type="action" required="false">
        <caption>On form error</caption>
        <description>Action to trigger when form errors</description>
      </property>
    </propertyGroup>
  </properties>
</widget>
